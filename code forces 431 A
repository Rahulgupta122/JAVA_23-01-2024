import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int ar[] = new int[4];
        ar[0] = sc.nextInt();
        ar[1] = sc.nextInt();
        ar[2] = sc.nextInt();
        ar[3] = sc.nextInt();
        String str = sc.next();
        long sum=0;
        for(int x=0;x<str.length();x++){
            char ch=str.charAt(x);
            sum+=ar[ch-'1'];
        }
        System.out.println(sum);
    }
}
