
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	     Scanner sc=new Scanner(System.in);
	     int a=sc.nextInt();
	     int c=0;
	     int []arr=new int[a];
	     for(int i=0;i<a;i++){
	         arr[i]=sc.nextInt();
	     }
	     for(int i=0;i<arr.length;i++){
	         c=c+arr[i];
	     }
	     float av=c/a;
	     System.out.println(av);
	}
}
